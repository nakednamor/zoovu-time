{
  "name": "Zoovu Time Track",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Tracking working times.",
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/get_started16.png",
      "32": "icons/get_started32.png",
      "48": "icons/get_started48.png",
      "128": "icons/get_started128.png"
    }
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "declarativeContent"
  ]
}
