{
    "extends": ["tslint:latest", "tslint-react", "tslint-eslint-rules", "tslint-config-prettier", "tslint-plugin-prettier"],
    "rules": {
        "prettier": true,
        "no-submodule-imports": false,
        "no-implicit-dependencies": false,
        "jsx-no-lambda": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "interface-name": false,
        "member-access": false,
        "member-ordering": false,
        "variable-name": false
    }
}
